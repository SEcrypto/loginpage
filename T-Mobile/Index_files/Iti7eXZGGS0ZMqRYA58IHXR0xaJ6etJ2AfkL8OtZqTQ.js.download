var screenIdleTime=0;$(document).ready(function(){var a=setInterval(incrementIdleTime,60000);$(this).mousemove(function(b){screenIdleTime=0});$(this).keypress(function(b){screenIdleTime=0})});function incrementIdleTime(){screenIdleTime=screenIdleTime+1;if(screenIdleTime>30){showIdleTimeoutPopup()}}function showIdleTimeoutPopup(){qvxp.messageWindow({status:"usererror",title:"Login Screen Idle for Too Long",message:"Click refresh to log in or quit to close the application.",cls:"tmo-user-error",btns:[{text:"Refresh",cls:"btn-primary pull-right",click:function(){window.location.reload()}},{text:"Quit",cls:"pull-left",click:function(){window.open("","_self");window.close()}}]});$(".modal-header .close").remove()};